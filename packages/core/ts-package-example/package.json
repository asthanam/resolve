{
  "name": "ts-package-example",
  "version": "0.22.5",
  "description": "ts-package-example",
  "engines": {
    "node": ">=10.9"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "test": "jest",
    "testw": "echo || exit 0"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/ts-package-example#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "jest": "25.1.0",
    "typescript": "3.8.2",
    "ts-jest": "25.2.1",
    "rimraf": "3.0.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)",
      "**/?(*.)+(spec|test).test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "es",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./es"
    },
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./src",
      "outDir": "./lib"
    }
  ]
}
