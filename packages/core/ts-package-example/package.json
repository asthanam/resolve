{
  "name": "ts-package-example",
  "version": "0.22.5",
  "description": "Provides the ZeroMQ API to reSolve packages.",
  "engines": {
    "node": ">=10.9"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "bugs": {
    "url": "https://github.com/reimagined/resolve/issues"
  },
  "scripts": {
    "prepare": "rimraf ./**/*.d.ts ./**/*.js ./**/*.js.map ./**/*.mjs ./**/*.mjs.map && tsc",
    "test": "echo || exit 0",
    "testw": "echo || exit 0"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/core/resolve-zeromq#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "jest": "25.1.0",
    "typescript": "3.8.2",
    "ts-jest": "25.2.1",
    "rimraf": "3.0.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.unit.test.[jt]s?(x)",
      "**/?(*.)+(spec|test).unit.test.[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  },
  "babelCompile": [
    {
      "sourceType": "ts",
      "moduleType": "mjs",
      "moduleTarget": "client",
      "inputDir": "./",
      "outDir": "./"
    },
    {
      "sourceType": "ts",
      "moduleType": "cjs",
      "moduleTarget": "client",
      "inputDir": "./",
      "outDir": "./"
    }
  ]
}
